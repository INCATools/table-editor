{
  "name": "table-editor",
  "version": "0.0.10",
  "description": "",
  "main": "app.js",
  "scripts": {
    "publish": "./publish.sh",
    "clean": "rm -rf dist/",
    "symlink": "(cd dist && ln -s . table-editor)",
    "dupindex": "cp dist/index.html dist/404.html",
    "build": "npm run clean && BUILD=production node_modules/.bin/webpack --bail && npm run dupindex",
    "devbuild": "npm run clean && BUILD=lproduction node_modules/.bin/webpack --bail && npm run dupindex && npm run symlink && npm run server",
    "dev": "npm run clean && BUILD=development node_modules/.bin/webpack-dev-server --port 8085 --devtool=source-map --progress --colors --content-base app",
    "server": "node_modules/.bin/http-server --p 8085 --cors -c-1 dist/",
    "test": "export BUILD=test && karma start"
  },
  "keywords": [
    "angular",
    "csv",
    "yaml",
    "table",
    "webpack",
    "es6"
  ],
  "author": "Daniel Keith <keithda@oregonstate.edu> (https://tislab.org)",
  "license": "ISC",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.21",
    "angular": "^1.7.6",
    "angular-fontawesome": "^1.0.0",
    "angular-resource": "^1.7.6",
    "angular-sanitize": "^1.7.6",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-grid": "^4.6.6",
    "bootstrap-sass": "^3.4.0",
    "chai": "^4.2.0",
    "file-saver": "^2.0.0",
    "font-awesome": "^4.7.0",
    "github-api": "^3.0.0",
    "js-yaml": "^3.12.1",
    "jsonformatter": "^0.7.0",
    "ng-file-upload": "^12.2.13",
    "ngstorage": "^0.3.11",
    "papaparse": "^4.6.3",
    "tetool": "0.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "angular-mocks": "^1.7.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
